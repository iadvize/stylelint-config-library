{
  "name": "@iadvize-oss/stylelint-config",
  "version": "0.1.1-beta.0",
  "description": "iAdvize stylelint rules",
  "main": "src/index.js",
  "scripts": {
    "lint:js": "eslint src/*",
    "bump-changelog": "VERSION=$(cat package.json | jq '.version') && remark CHANGELOG.md -o --use @jarrodldavis/changelog-version-bump=version:$VERSION && git add CHANGELOG.md"
  },
  "homepage": "https://github.com/iadvize/stylelint-config-library#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iadvize/stylelint-config-library.git"
  },
  "keywords": [
    "iAdvize",
    "stylelint"
  ],
  "bugs": {
    "url": "https://github.com/iadvize/stylelint-config-library/issues"
  },
  "author": "iAdvize developers",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@iadvize-oss/eslint-config": "0.1.1",
    "@jarrodldavis/remark-changelog-version-bump": "^0.1.1",
    "eslint": "^6.8.0",
    "remark-cli": "^7.0.1",
    "typescript": "^3.7.5"
  },
  "peerDependencies": {
    "stylelint": "^13.2.0"
  }
}
